{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "no-restricted-modules": "off",
        "no-use-before-define": "off",
        "no-restricted-globals": "off",
        "no-shadow": "off",
        "semi":"error",
        "func-names": "off",
        "radix": "off"
    },
    "env": {
        "node": true
    }
}